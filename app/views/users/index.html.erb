<table>
<% @users.each do |user| %>
  <tr>
  <td><%= user.email %></td>
  <td><%= link_to 'Show', user_path(user) %></td>
  <td><%= link_to 'Edit', edit_user_registration_path(:id => user) %></td>
  <td><%= link_to 'Delete', user_path(user), :confirm => 'Delete?', :method => :delete %></td>
  </tr>
<% end  %>
</table>
